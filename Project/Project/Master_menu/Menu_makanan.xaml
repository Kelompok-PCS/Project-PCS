<Window x:Class="Project.Master_menu.Menu_makanan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project.Master_menu"
        mc:Ignorable="d"
        Title="Menu Makanan" Height="450" Width="800" WindowState="Maximized">
    <Grid>
        <Button x:Name="btnFilter" Margin="708,38,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="50" Height="26" Click="btnFilter_Click" Panel.ZIndex="1">
            <Image Source="/Resources/search-icon.png" Height="22" Margin="4,0,6,0" Panel.ZIndex="0" />
        </Button>
        <TextBox x:Name="tbFilter" HorizontalAlignment="Left" Height="26" Margin="588,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Filter By" HorizontalAlignment="Left" Margin="536,38,0,0" VerticalAlignment="Top" Background="#FFE0A800"/>
        <Button x:Name="btnInsert" Content="Insert New Menu" HorizontalAlignment="Left" Margin="201,38,0,0" VerticalAlignment="Top" Width="122" Background="#FF0069D9" Foreground="#FFF7EFEF" Height="26" Click="btnInsert_Click"/>
    </Grid>
</Window>
